{
    "name": "paf/code-template",
    "description": "code-template",
    "repositories": [
        {"type": "vcs", "url": "git@git.ipo.com:hf-dev-0/lightservice.git"},
        {"type": "vcs", "url": "git@git.ipo.com:hf-estate-specialist/estate-lib.git"},
        {
            "name": "paf/l5-feather",
            "type": "vcs",
            "url": "git@git.ipo.com:hf-dev-0/l5-feather.git"
        },
        {
            "type": "vcs",
            "url": "http://git.ipo.com/hf-dev-fe/feather-view.git"
        },
        {
            "type": "vcs",
            "url": "git@git.ipo.com:ananzu/ananzu-vendor.git"
        },
        {
            "type": "vcs",
            "url": "git@git.ipo.com:hf-dev-0/exception.git"
        }
    ],
    "type": "project",
    "require": {
        "paf/lightservice": "~2.0",
        "paf/estate": "~2.0",
        "paf/l5-feather": "v0.1.0",
        "ananzu/ananzu": "1.1.*",
        "paf/exception": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-update-cmd": ["rm -rf vendor/**/**/.git"],
        "post-install-cmd": ["rm -rf vendor/**/**/.git"],
        "update-estate": ["rm -rf vendor/paf/estate/ && composer update paf/estate"]
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
